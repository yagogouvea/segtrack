{
  "name": "segtrack-backend",
  "version": "1.0.0",
  "description": "Backend do sistema SEGTRACK",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "build": "tsc -p tsconfig.json && tsc-alias",
    "start": "node dist/server.js",
    "start:prod": "node dist/server.js",
    "postinstall": "prisma generate",
    "prestart": "prisma migrate deploy",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prod:pm2": "pm2 start ecosystem.config.js --env production",
    "prod:stop": "pm2 stop ecosystem.config.js",
    "swagger:generate": "tsoa spec-and-routes",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/pg": "^8.15.4",
    "aws-sdk": "^2.1569.0",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.1",
    "pg": "^8.16.0",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.825.0",
    "@prisma/client": "^5.10.2",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.19",
    "@types/pino": "^7.0.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "pm2": "^5.4.3",
    "prisma": "^5.10.2",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "ts-prune": "^0.10.3",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
